#ifndef WIN32_LEAN_AND_MEAN
#define WIN32_LEAN_AND_MEAN
#endif
#include <windows.h>

IDI_ICON1               ICON    DISCARDABLE     "nmclauncher.ico"
1 RT_MANIFEST "nmclauncher.manifest"

VS_VERSION_INFO VERSIONINFO
FILEVERSION     @Launcher_VERSION_NAME4_COMMA@
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_APP
BEGIN
        BLOCK "StringFileInfo"
        BEGIN
                BLOCK "000004b0"
                BEGIN
                        VALUE "CompanyName", "MultiMC & Prism Launcher & PollyMC Contributors"
                        VALUE "FileDescription", "NMC Launcher"
                        VALUE "FileVersion", "@Launcher_VERSION_NAME4@"
                        VALUE "ProductName", "NMC Launcher"
                        VALUE "ProductVersion", "@Launcher_VERSION_NAME4@"
                END
        END
        BLOCK "VarFileInfo"
        BEGIN
                VALUE "Translation", 0x0000, 0x04b0 // Unicode
        END
END
